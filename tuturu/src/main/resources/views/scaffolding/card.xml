<rutu:doNothing>
    <div class="square square-7"></div>
    <div class="square square-8"></div>
    <div class="card card-register">
        <div class="card-header" style="padding: 0 0 0px;">
            <h4 class="card-title" style="background-image: url(../assets/img/square1.png);
                                            background-position: bottom;
                                            background-size: cover;
                                            margin-left: 0px;
                                            font-size: 2.2em;
                                            text-transform: capitalize;
                                            padding: 10px;">
                <rutu:value-of select="firstName"></rutu:value-of>
                <rutu:value-of select="lastName"></rutu:value-of>
            </h4>
        </div>
        <div class="card-body">
            <h3>Groupe :
                <rutu:value-of select="group"></rutu:value-of>
            </h3>
            <h4>Identifiant :
                <rutu:value-of select="id"></rutu:value-of>
            </h4>
        </div>
        <div class="card-footer">
            <div class="row">
                <a href="${id} tuturu ${lastName} lolilol" class="btn btn-primary btn-round btn-lg">
                    <i class="tim-icons icon-double-right"></i>
                    Afficher
                </a>
                <a href="javascript:void(0)" class="btn btn-info btn-round btn-lg">
                    <i class="tim-icons icon-pencil"></i>
                    Editer
                </a>
                <a href="javascript:void(0)" class="btn btn-danger btn-round btn-lg">
                    <i class="tim-icons icon-trash-simple"></i>
                </a>
            </div>
        </div>
    </div>
</rutu:doNothing>